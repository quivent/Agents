{
  "name": "agentard-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "node scripts/generate-content-json.js",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "test:performance": "node scripts/performance-test.js",
    "test:lighthouse": "npx lighthouse http://localhost:3000 --output json --output html --output-path ./lighthouse-results",
    "optimize:content": "node scripts/enhance-content-metadata.js",
    "clean": "rm -rf .next out",
    "deploy:vercel": "vercel --prod",
    "prepare": "npm run type-check"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^15.5.3",
    "@types/mdx": "^2.0.13",
    "gray-matter": "^4.0.3",
    "next": "15.5.3",
    "next-mdx-remote": "^5.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
